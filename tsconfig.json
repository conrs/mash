{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 22",
    "_version": "22.0.0",
  
    "compilerOptions": {
      "lib": ["es2023", "DOM"],
      "module": "nodenext",
      "target": "es2022",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node16"
    }
  }